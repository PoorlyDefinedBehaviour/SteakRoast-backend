{
  "name": "steakroastbackend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env ENVIRONMENT=TEST jest --watch",
    "dev": "ts-node-dev --inspect -- src/main.ts",
    "start": "ts-node src/main.ts",
    "gen-schema-types": "gql2ts src/graphql/**/*.graphql -o src/types/graphql.d.ts",
    "typeorm-migration-generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n MigrationName",
    "typeorm-migration-run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm-migration-revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [],
  "author": "PoorlyDefinedBehaviour",
  "repository": {
    "type": "git",
    "url": "https://github.com/PoorlyDefinedBehaviour/SteakRoast-backend.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.15",
    "@types/node": "^8.0.29",
    "@types/yup": "^0.26.20",
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "faker": "^4.1.0",
    "gql2ts": "^1.10.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/express-graphql": "^0.8.0",
    "@types/glob": "^7.1.1",
    "@types/mongodb": "^3.1.28",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-graphql": "^0.8.0",
    "glob": "^7.1.4",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.5",
    "merge-graphql-schemas": "^1.5.8",
    "mongodb": "^3.2.7",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.18",
    "yup": "^0.27.0"
  }
}
